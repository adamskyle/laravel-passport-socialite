{
    "name": "schedula/laravel-passport-socialite",
    "description": "The missing laravel passport feature for social authentication",
    "keywords" : [ "laravel", "social-login", "laravel-passport", "passport", "laravel-socialite", "socialite"],
    "license": "MIT",
    "authors": [
        {
            "name": "Anand Siddharth",
            "email": "anandsiddharth21@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/passport": "^10.0",
        "laravel/socialite": "^4.0 || ^5.0",
        "schedula/league-oauth2-social": "dev-laravel-8-support as 1.0",
        "illuminate/support": "~5.7 || ^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Schedula\\Laravel\\PassportSocialite\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Schedula\\Laravel\\PassportSocialite\\PassportSocialiteServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mraap/league-oauth2-social"
        }
    ]
}
